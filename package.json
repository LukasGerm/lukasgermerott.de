{
  "private": true,
  "name": "lukasgermerott.de",
  "description": "Homepage of lukas germerott",
  "license": "GPL",
  "version": "1.2.1",
  "scripts": {
    "build": "yarn run build:css && remix build",
    "dev": "node -r dotenv/config node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "lint": "eslint -c .eslintrc.js app",
    "prettier": "prettier --write app/**/*.ts app/**/*.tsx",
    "watch:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "test:unit": "yarn run lint && yarn run build:css && jest"
  },
  "dependencies": {
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/forms": "^0.5.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "front-matter": "^4.0.2",
    "highlight.js": "^11.6.0",
    "i18next": "^22.4.6",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1",
    "remix": "^1.7.2",
    "remix-i18next": "^5.0.0",
    "remix-utils": "^6.0.0",
    "showdown": "^2.1.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/showdown": "^2.0.0",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^29.1.2",
    "eslint": "^8.25.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-junit": "^14.0.1",
    "postcss": "^8.4.17",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "18.x"
  },
  "sideEffects": false
}
